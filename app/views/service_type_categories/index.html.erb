<p id="notice"><%= notice %></p>

<h1>Service Type Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_type_categories.each do |service_type_category| %>
      <tr>
        <td><%= service_type_category.name %></td>
        <td><%= service_type_category.order %></td>
        <td><%= link_to 'Show', service_type_category %></td>
        <td><%= link_to 'Edit', edit_service_type_category_path(service_type_category) %></td>
        <td><%= link_to 'Destroy', service_type_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service Type Category', new_service_type_category_path %>
